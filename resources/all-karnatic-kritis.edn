 {:kriti "broohi mukundEti",
  :ragam "kurinji",
  :composer "Sadaashiva BruhmEndrar",
  :language nil,
  :taalam "aadi",
  :lyrics
  ("<p>pallavi</p>"
   "<p>brUhi mukundEti rasanE brUhi mukundEti rasanE (brUhi)</p>"
   "<p>caraNam 1</p>"
   "<p>kEshava mAdhava gOvindEti krSNAnanda sadAnandEti (brUhi)<br /></p>"
   "<p>caraNam 2</p>"
   "<p>rAdhA ramaNa harE rAmEti rAjIvAkSa ghanashyAmEti (brUhi)</p>"
   "<p>caraNam 3</p>"
   "<p>garuDa gamana nandaka hastEti khaNDita dashakandhAmastEti (brUhi)</p>"
   "<p>caraNam 4</p>"
   "<p>akrUra priya cakradharEti hamsa niranjana kamsa harEti (brUhi) <br /></p>"),
