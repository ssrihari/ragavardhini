 {:kriti "ennannaiyE",
  :ragam "kamaas",
  :composer "H.N. Muttayyah Bhaagavatar\\n\\n",
  :language "Tamil",
  :taalam "Eka",
  :lyrics
  ("<p>pallavi</p>"
