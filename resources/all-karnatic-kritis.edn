  :taalam "aadi",
  :lyrics
  ("<p>\\r<br />pallavi \\r</p>"
   "<p>\\r<br />naagalingam namaami satatam\\r<br />naama roopa prapancaadeeta lingam\\r<br />(naagalingam)\\r</p>"
   "<p>\\r<br />anupallavi \\r</p>"
