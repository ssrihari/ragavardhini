  :language nil,
  :taalam "mishra",
  :lyrics
  ("<p>pallavi</p>"
   "<p>Narayanaaya Namo Maadhavayanamo</p>"
   "<p>anupallavi</p>"
   "<p>Naagendra Sayanaaya Nandhithaakila Lokhaya</p>"
   "<p>caraNam 1</p>"
   "<p>Naabhi kamala janitha vidhi mukhaaya<br />Noothana Jala thaaraapaya<br />Naanaa saraasara naaree nara roopaaya<br />Naaradhaadhi muni vandhyaanantha gana Mayaaya</p>"
   "<p>caraNam 2</p>"
