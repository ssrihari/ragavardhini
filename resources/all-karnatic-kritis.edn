  ("<p>\\r<br />pallavi \\r</p>"
   "<p>\\r<br />anAthuDanu gAnu; rAma nE!   \\r<br />(anAthuDanu)\\r</p>"
   "<p>\\r<br />anupallavi\\r</p>"
   "<p>\\r<br />anAthuDavu nIvani niga majnyula sanAtanula mATa vinnAnu;\\r<br />nE   \\r<br />(anAthuDanu)\\r</p>"
   "<p>\\r<br />caraNam\\r</p>"
