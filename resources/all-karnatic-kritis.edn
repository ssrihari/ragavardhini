 {:kriti "entO mahaanubhaavuDavu",
  :ragam "shankaraabharaNam",
  :composer "Badraacala Raamadaas",
  :language nil,
  :taalam "aadi",
  :lyrics
  ("<p>pallavi</p>"
   "<p>entO mahAnubhAvuDavu nIvenO cakkani dEvuDavu rAma</p>"
