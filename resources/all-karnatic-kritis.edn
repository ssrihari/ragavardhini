  :taalam "tripuTa",
  :lyrics
  ("<p>pallavi</p>"
   "<p>entanucu varniNtunE I indirA ramaNuni nE</p>"
   "<p>anupallavi</p>"
   "<p>santatamu nammu sajjanula A santa dIrcu vasanta sukumAruni</p>"
