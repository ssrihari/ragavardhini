  :language "Telugu",
  :taalam "caapu",
  :lyrics
  ("<p>pallavi</p>"
   "<p>innALLavale vintasEyakE nIvADa nipuDaiti dharmAmbikE</p>"
   "<p>anupallavi</p>"
